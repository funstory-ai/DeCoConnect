{
  "name": "Janus",
  "version": "0.1.0",
  "description": "Find and mark translation errors.",
  "permissions": ["tabs", "*://*.webnovel.com/*"],
  "content_scripts": [
    {
      "matches": ["*://*.babelnovel.com/*"],
      "js": ["js/jquery-3.4.1.min.js", "js/side-comments.js", "js/janus.js"],
      "css": ["css/side-comments.css", "css/themes/default-theme.css"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "images/janus_16.png",
    "32": "images/janus_32.png",
    "48": "images/janus_48.png",
    "128": "images/janus_128.png"
  },
  "manifest_version": 2
}